                            <list_item>
                                <paragraph>‘course_id’: unicode,</paragraph>
                            </list_item>
                            <list_item>
                                <paragraph>‘item_id’: unicode</paragraph>
                            </list_item>
                        </bullet_list>
                    </block_quote>
                </desc_content>
            </desc>
